<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>
  <appSettings>
    <!-- Valor que define a faixa de Ip utilizada na rede -->
    <add key="faixa_ip" value="10.0.7." />
    <!-- Valor para o ciclo de varredura da rede 1s=1000 -->
    <add key="ciclo" value="3000" />
    <!-- Valor arbitrado para Inicio do range de Ip -->
    <add key="faixa_ini" value="1" />
    <!-- Valor arbitrado para fim do range de Ip -->
    <add key="faixa_fim" value="254" />
    <!-- Oid padrão para captura do Contador geral do dispositivo -->
    <add key="oid_contador" value="" />
    <!-- Oid padrão para captura do Nr de série do dispositivo -->
    <add key="oid_serial" value="" />
    <!-- Oid padrão para captura do Modelo do dispositivo -->
    <add key="oid_modelo" value="" />
    <!-- Primeiro OID do Range de impressoras. Serve para idenfificar se o dispositivo é uma impressora -->
    <add key="oid_impressora" value="" />
    <!-- Oid padrão para captura do Mac Address do dispositivo -->
    <add key="oid_mac" value="" />
    <!-- Id do cliente onde o serviço está instalado -->
    <add key="Id_cliente" value="99b062f8-d415-11e6-bfdc-00155d6eac04" />
    <!-- define o tempo esperado para a leitura do snmp-->
    <add key="TimeOut_snpm" value="2400" />

    <!-- define Nr Contrato-->
    <add key="Nr_contrato" value="q1w2e3r4t5" />

     <!-- define Caminho Certificado fechar com "\"-->
    <add key="Path_Cert" value="D:\googleDrive\Sistemas\Desenvolvimento\Reis\Certificado\" />
    <!-- define IP Principal-->
    <add key="IP" value="10.0.1.82" />
    
    <!-- String de conexão do db -->
    <!-- <add key="Conn_db" value="server=VM-WinServer;user id=root;password=123456;persistsecurityinfo=True;database=tarifador" />-->
    <add key="Conn_db" value="server=192.168.0.89;user id=reis;password=Reis2015;persistsecurityinfo=True;database=tarifador" />
    <add key="Conn_db_SQLite" value="Data Source=D:\googleDrive\Sistemas\Desenvolvimento\Reis\dbSqLite\tarifador.db;Version=3;" />
    <!--
Para as colunas vazias o sitema utiliza valores internos pré-definidos
faixa de ip =192.168.0
ciclo = 1000
faixa_ini = 10
faixa_fim = 255 
nr serie = 1.3.6.1.2.1.43.5.1.1.17.1
modelo = 1.3.6.1.2.1.25.3.2.1.3.1
mac address = 1.3.6.1.2.1.2.2.1.6.1
